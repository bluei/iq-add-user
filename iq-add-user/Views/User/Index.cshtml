@model IEnumerable<iq_add_user.Models.S_USER_GENERAL>

@{
    ViewBag.Title = "Index";
}

<h2>IQMS Users</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table table-condensed table-striped table-responsive">
    <tr>
        <th>
            User Name
        </th>
        <th>
            Shutdown Sec
        </th>
        <th>
            Email
        </th>
        <th>
            Emp Id
        </th>
        <th>
            Lang Code
        </th>
        <th>
            Force Pwd Change
        </th>
        <th>
            Eplant
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.USER_NAME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AUTO_SHUT_TIME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EMAIL)
        </td>
        
        <td>
            @Html.DisplayFor(modelItem => item.PR_EMP_ID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LANGUAGE_CODE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FORCE_PASSWORD_CHANGE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EPLANT.NAME)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.USER_NAME }) |
            @Html.ActionLink("Details", "Details", new { id=item.USER_NAME }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.USER_NAME })
        </td>
    </tr>
}

</table>
